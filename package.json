    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e:install": "cd e2e/a9 && npm install && npm run postinstall",
        "e2e:spread": "P=e2e/a9/node_modules/ngrx-correlation-id && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-correlation-id/. $P",
        "e2e:test": "cd e2e/a9 && npm run e2e",
        "build:cid": "ng build ngrx-correlation-id --prod",
        "test:cid": "ng test ngrx-correlation-id",
        "test:cid:debug": "ng test ngrx-correlation-id -- --watch --browsers=Chrome",
        "publish": "cd dist/ngrx-correlation-id && npm publish",
        "postinstall": "ngcc"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:satanTime/ngrx-correlation-id.git"
    },
    "author": "MG. <m@sudo.eu>",
