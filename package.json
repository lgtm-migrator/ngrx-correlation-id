        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.12.42",
        "codelyzer": "^5.1.2",
        "commitlint": "^8.3.5",
        "coveralls": "^3.1.0",
        "husky": "^4.2.5",
